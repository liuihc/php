<?php
	define('PATH', dirname(dirname(dirname(dirname(__FILE__)))) . '/');
	require_once(PATH.'wp-blog-header.php');  
	global $wpdb;
	
	$fivesdrafts = $wpdb->get_results("SELECT * FROM wp_enroll",ARRAY_A);

	echo ("<table border='1'> <tr> <th>姓名</th> <th>网络地址</th> <th>报名时间</th> </tr>");
	foreach ( $fivesdrafts as $fivesdraft ) 
	{
		echo ("<tr> <td>$fivesdraft[name]</td> <td>$fivesdraft[ip]</td> <td>$fivesdraft[time]</td> </tr>");
	}
	echo ("</table>");
		//echo ("<P>" . $fivesdraft['name'] . $fivesdraft['ip'] .$fivesdraft['time'] . "</P>");
	 //[phpcode file="filename"]
?>
